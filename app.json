{
    "name": "LazyPrincess",
    "description": "When you going to send file on telegram channel this bot will save that in database, So you can search that easily in inline mode",
    "stack": "container",
    "keywords":[
      "pyrogram",
      "media",
      "search",
      "channel",
      "index",
      "inline"
    ],
    "website": "https://github.com/patelboss/Imdb",
    "repository": "https://github.com/patelboss/Imdb",
    "env": {
        "TELEGRAM_BOT_TOKEN": {
            "description": "Your bot token.",
            "required": true
        }
    },
    "addons": [],
    "buildpacks": [{
        "url": "heroku/python"
    }],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
